{
    "name": "@bewinxed/wallet-adapter-svelte-ui",
    "version": "0.2.1",
    "author": "bewinxed <bewinxed@gmail.com>",
    "repository": "https://github.com/bewinxed/wallet-adapter-svelte-ui",
    "keywords": [
        "solana",
        "svelte",
        "svelte 5",
        "runes",
        "wallet",
        "adapter"
    ],
    "license": "Apache-2.0",
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist",
        "!dist/**/*.test.*",
        "!dist/**/*.spec.*"
    ],
    "engines": {
        "node": ">=18"
    },
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "svelte": "./dist/index.js"
        }
    },
    "scripts": {
        "clean": "shx mkdir -p lib && shx rm -rf lib",
        "dev": "vite dev",
        "build": "vite build && npm run package",
        "preview": "vite preview",
        "package": "svelte-kit sync && svelte-package && publint",
        "prepublishOnly": "npm run package",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "lint": "prettier --check . && eslint .",
        "format": "prettier --write ."
    },
    "peerDependencies": {
        "@solana/web3.js": "^2.0.0-rc.1",
        "svelte": "^5.0.0-next.1",
        "@bewinxed/wallet-adapter-svelte": "^0.16.1",
        "@solana/wallet-adapter-wallets": "^0.19.32"
    },
    "dependencies": {
        "@bewinxed/wallet-standard-wallet-adapter-svelte": "^1.1.6"
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^3.2.5",
        "@sveltejs/adapter-node": "^5.2.5",
        "@sveltejs/kit": "^2.6.3",
        "@sveltejs/package": "^2.3.5",
        "@sveltejs/vite-plugin-svelte": "^4.0.0-next.7",
        "@types/eslint": "^9.6.1",
        "autoprefixer": "^10.4.20",
        "eslint": "^9.12.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-svelte": "^2.44.1",
        "globals": "^15.10.0",
        "postcss": "^8.4.47",
        "postcss-cli": "^11.0.0",
        "prettier": "^3.3.3",
        "prettier-plugin-svelte": "^3.2.7",
        "prettier-plugin-tailwindcss": "^0.6.8",
        "publint": "^0.1.16",
        "shx": "^0.3.4",
        "svelte": "^5.0.0-next.262",
        "svelte-check": "^3.8.6",
        "svelte-sonner": "^0.3.28",
        "tailwindcss": "^3.4.13",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.6.2",
        "typescript-eslint": "^8.8.1",
        "vite": "^5.4.8",
        "vite-plugin-node-polyfills": "^0.22.0",
        "vite-plugin-scope-tailwind": "^2.0.1"
    },
    "svelte": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module"
}
